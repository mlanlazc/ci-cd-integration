{
  "sdkName": "hello-world-sdk",
  "apiVersion": "1.0.0",
  "apiName": "hello-world-sdk-api",
  "specFilePath": "./api.json",
  "baseUrl": "https://ci-cd-integration.onrender.com",
  "languages": [
    "python"
  ],
  "auth": [
    "bearer"
  ],
  "customizations": {
    "includeOptionalSnippetParameters": true,
    "authentication": {
      "access": {
        "prefix": "Bearer"
      }
    },
    "devContainer": true,
    "generateEnv": true,
    "inferServiceNames": false,
    "injectedModels": [],
    "license": {
      "type": "MIT"
    },
    "responseHeaders": false,
    "retry": {
      "enabled": true,
      "maxAttempts": 3,
      "retryDelay": 150
    }
  },
  "mcp": {
    "generate": false
  },
  "languageOptions": {
    "python": {
      "alwaysInitializeOptionals": false,
      "pypiPackageName": "",
      "githubRepoName": "ci-cd-integration",
      "ignoreFiles": [],
      "sdkVersion": "1.0.0",
      "liblabVersion": "2"
    }
  },
  "publishing": {
    "githubOrg": "mlanlazc"
  }
}